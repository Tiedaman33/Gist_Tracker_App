{
    "version": 2,
    "builds": [
      {
        "src": "next.config.js",
        "use": "@vercel/next"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ],
    "env": {
      "GITHUB_API_URL": "https://api.github.com",
      "GITHUB_TOKEN": "@github_token"
    }
  }